#Creating a list and deleting the duplicate elements

def remove_duplicates(list):
	newlist=[]
	for no in list:
		if no not in newlist:
			newlist.append(no)
	
	print("List without duplicates",newlist)

i=input("\n Enter the elements of list separated by commas \n")
list=i.split(",")
print("Actual list=",list)
remove_duplicates(list)
